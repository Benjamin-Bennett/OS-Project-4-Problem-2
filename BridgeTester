
public class BridgeTester
{
    public static void main(String[] Args){
            
    Car[] cars = new Car[10];
    Bridge myBridge = new Bridge();
    for (int i = 0; i < 5; i++){
        cars[i] = new Car(myBridge, "West");
        
    }
    for (int i = 5; i < cars.length; i++){   
        cars[i] = new Car(myBridge, "East");
        
    }
    for (int i = 0; i < cars.length; i++){   
        cars[i].start();
    }
}
}
