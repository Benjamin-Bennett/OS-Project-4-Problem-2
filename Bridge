import java.util.concurrent.Semaphore;

public class Bridge
{
    private Semaphore bridgeKey;
    
    
    
    public Bridge(){
        
     bridgeKey = new Semaphore(1);   
    }
    public void cross(){
        
     try {
         bridgeKey.acquire();   
        }
     catch (InterruptedException e) {}
    }
    public void done(){
       bridgeKey.release();
    }
}
